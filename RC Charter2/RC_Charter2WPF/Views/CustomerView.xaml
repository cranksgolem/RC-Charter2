<UserControl
	x:Class="RC_Charter2WPF.Views.CustomerView"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:local="clr-namespace:RC_Charter2WPF.Views"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:parts="clr-namespace:RC_Charter2WPF.Views.Parts"
	xmlns:viewModels="clr-namespace:RC_Charter2WPF.ViewModels"
	d:DesignHeight="450"
	d:DesignWidth="800"
	mc:Ignorable="d">
	<UserControl.Resources>
		<viewModels:CustomerViewModel x:Key="CustomerViewModel" />
	</UserControl.Resources>
	<Grid DataContext="{StaticResource CustomerViewModel}">
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="1*" />
			<ColumnDefinition Width="3*" />
		</Grid.ColumnDefinitions>
		<Grid Grid.Column="0">
			<parts:CustomerList />
		</Grid>
	</Grid>
</UserControl>
