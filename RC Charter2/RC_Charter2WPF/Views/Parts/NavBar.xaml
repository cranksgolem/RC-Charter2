<UserControl
	x:Class="RC_Charter2WPF.Views.Parts.NavBar"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:local="clr-namespace:RC_Charter2WPF.Views.Parts"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	d:DesignHeight="450"
	d:DesignWidth="800"
	mc:Ignorable="d">
	<Grid>
		<Grid.ColumnDefinitions>
			<ColumnDefinition />
			<ColumnDefinition />
			<ColumnDefinition />
			<ColumnDefinition />
		</Grid.ColumnDefinitions>
		<Button x:Name="BtnCustomerView"
			Grid.Column="0"
			Margin="4"
			Padding="4"
			Content="Customer" Click="BtnCustomerView_Click" />
		<Button x:Name="BtnEmployeeView"
			Grid.Column="1"
			Margin="4"
			Padding="4"
			Content="Employee" Click="Button_Click" />
		<Button
			Grid.Column="2"
			Margin="4"
			Padding="4"
			Content="Aircraft" />
		<Button
			Grid.Column="3"
			Margin="4"
			Padding="4"
			Content="Charter Trip" />
	</Grid>
</UserControl>
